<div class="container">
  <div class="row">
    <div class="col-1"></div>
    <div class="col-3">
      <h5 class="daimei">ジャンル編集</h5>
    </div>
  </div>

  <div class="box1">
    <%= form_with model: @genre, url: admin_genre_path, method: :patch, local: true do |f| %>
      <table>
        <tr>
          <th>ジャンル名</th>
          <td><%= f.text_field :name %></td>
          <td><%= f.submit '変更を保存', class: "btn btn-success" %></td>
      </tr>
      </table>
    <% end %>
  </div>
</div>